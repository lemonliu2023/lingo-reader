  },
  "main": "./src/index.node.js",
  "module": "./src/index.node.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "browser": "./dist/index.browser.mjs",
  "scripts": {
    "build": "rollup -c"
  },
  "dependencies": {
    "@lingo-reader/shared": "workspace:^",
    "fflate": "catalog:",
    "jszip": "^3.10.1"
